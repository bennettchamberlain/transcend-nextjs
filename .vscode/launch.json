  "version": "0.2.0",
  "configurations": [
    {
      "type": "dart",
      "request": "launch",
      "name": "Launch main_development.dart (Web)",
      "program": "lib/main_development.dart",
      "args": ["--web-port=5554"],
      "deviceId": "web-server"
